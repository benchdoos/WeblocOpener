{
  "browsers": [
    {
      "browser": {
        "name": "Chromium",
        "call": "chromium %site",
        "incognito": "chromium %site --incognito"
      }
    },
    {
      "browser": {
        "name": "Google Chrome",
        "call": "google-chrome %site",
        "incognito": "google-chrome %site --incognito"
      }
    },
    {
      "browser": {
        "name": "Firefox",
        "call": "firefox %site",
        "incognito": "firefox --private-window %site"
      }
    },
    {
      "browser": {
        "name": "Opera",
        "call": "opera %site",
        "incognito": "opera --private %site"
      }
    },
    {
      "browser": {
        "name": "Yandex",
        "call": "yandex-browser %site",
        "incognito": "yandex-browser -incognito %site"
      }
    }
  ]
}